<h2 class="ui header">
  <i class="folder open icon"></i>
  <div class="content">
    {{ project.name }}
    <div class="sub header"></div>
  </div>
</h2>
<h3 class="ui header">Stories</h3>
<a id="add-story-bt" class="ui button" data-ajax="project/{{ project.id }}/addStory">
	<i class="ui plus icon"></i>
	Add Story in project...
</a>
<a id="dashboard-bt" class="ui teal button" data-ajax="project/{{ project.id }}/board">
    <i class="calendar alternate outline icon"></i>
    Dashboard
</a>
<a href="{{ path('projects') }}" id="back-to-project-bt" class="ui button nav-stories" data-ajax="projects">
	<i class="caret square left icon"></i>
	Back to projects
</a>
<div class="ui divider"></div>

{{ q["list-stories"] | raw }}
<div id="board-div">

</div>
<div id="frm">
    <div id="frm-submit"></div>
</div>

{{ script_foot | raw }}